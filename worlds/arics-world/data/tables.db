ck.svg","documentCollection":"","documentId":null}],"formula":"1d100","replacement":true,"displayRoll":true,"folder":null,"sort":0,"flags":{},"ownership":{"default":0,"HvFwU8NyHMRPfUiD":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.286","createdTime":null,"modifiedTime":1664502125616,"lastModifiedBy":"9xy2ctfjHpL7zOTp"}}
