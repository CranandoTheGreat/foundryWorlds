:3,"characterClassId":51533706,"spellLevel":2,"ability":"int","mod":4,"dc":14,"cantripBoost":false,"overrideDC":false,"id":2323,"entityTypeId":435869154,"healingBoost":0,"usesSpellSlot":true},"sources":[{"sourceId":1,"pageNumber":null,"sourceType":2},{"sourceId":2,"pageNumber":null,"sourceType":1}],"tags":["Damage"]},"betterRolls5e":{"quickVersatile":{"altValue":true},"quickCharges":{"value":{"use":true,"resource":true},"altValue":{"use":true,"resource":true}}}}}],"effects":[],"folder":null,"sort":0,"permission":{"default":0,"uhpsnwcY44E4KDz0":3,"FSjNcA4sVDsBlRaB":3},"flags":{"ddbimporter":{"dndbeyond":{"url":"https://www.dndbeyond.com/profile/RKInspector/characters/40592204","apiEndpointUrl":"https://character-service.dndbeyond.com/character/v5/character/40592204","characterId":"40592204","totalLevels":3,"proficiencies":null,"proficienciesIncludingEffects":null,"roUrl":"https://dndbeyond.com/characters/40592204","characterValues":null,"templateStrings":null,"effectAbilities":null,"abilityOverrides":null},"compendium":false,"acEffects":[{"changes":[{"key":"data.attributes.ac.value","value":"11 + @abilities.dex.mod","mode":5,"priority":15}],"duration":{"seconds":null,"startTime":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"origin":"AC","label":"AC Base Armor - Racial (Unarmored): 11","tint":"","disabled":false,"transfer":true,"selectedKey":[],"icon":"icons/svg/shield.svg","flags":{"dae":{"transfer":true,"armorEffect":true},"ddbimporter":{"disabled":false,"characterEffect":true}}}],"baseAC":11,"autoAC":{"flat":null,"calc":"default","formula":""},"overrideAC":{"flat":11,"calc":"default","formula":""},"syncItemReady":true,"syncActionReady":true},"dnd5e":{"powerfulBuild":false,"savageAttacks":false,"elvenAccuracy":false,"halflingLucky":false,"initiativeAdv":false,"initiativeAlert":false,"jackOfAllTrades":false,"weaponCriticalThreshold":20,"observantFeat":true,"remarkableAthlete":false,"reliableTalent":false,"diamondSoul":false,"meleeCriticalDamage